{
  "name": "@bricktowers/rwa-ui",
  "version": "0.1.0",
  "author": "Brick Towers",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "http-server --port 8080 ./dist --proxy 'http://127.0.0.1:8080?'",
    "prepack": "yarn build",
    "copy-rwa-keys": "mkdir -p ./dist/keys && mkdir -p ./dist/zkir && cp -r ../rwa-contract/dist/managed/midnight-rwa/keys/* ./dist/keys && cp -r ../rwa-contract/dist/managed/midnight-rwa/zkir/* ./dist/zkir",
    "copy-token-keys": "mkdir -p ./dist/keys && mkdir -p ./dist/zkir && cp -r ../tbtc-contract/dist/managed/tbtc/keys/* ./dist/keys && cp -r ../tbtc-contract/dist/managed/tbtc/zkir/* ./dist/zkir",
    "build": "tsc && vite build --mode testnet && yarn copy-token-keys && yarn copy-rwa-keys",
    "buildAndRestart": "yarn build && yarn start",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@bricktowers/rwa-api": "*",
    "@bricktowers/rwa-contract": "*",
    "@bricktowers/tbtc-contract": "*",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "buffer": "^6.0.3",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.17.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "^8.52.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-react": "^7.33.2",
    "fast-check": "^3.23.2",
    "http-server": "^14.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.4.11",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.2.1"
  }
}
